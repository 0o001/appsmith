{
  "presets": ["env", "react"],
  "plugins": [
    "istanbul",
    [
      "module-resolver",
      {
        "root": ["./instrumented"],
        "alias": {
          "utils": "./instrumented/utils/",
          "test/*": ["../test/*"],
          "underscore": "lodash",
          "constants": "./instrumented/constants/",
          "components": "./instrumented/components/",
          "selectors": "./instrumented/selectors/",
          "reducers": "./instrumented/reducers/",
          "actions": "./instrumented/actions/",
          "api": "./instrumented/api/",
          "assets": "./instrumented/assets/",
          "sagas": "./instrumented/sagas/",
          "@appsmith": "./instrumented/ee"
        }
      }
    ]
  ]
}
